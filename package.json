  "scripts": {
    "install:both": "yarn && cd frontend && yarn  && cd ../backend && pip install -r requirements.txt",
    "dev": "source .venv/bin/activate && concurrently \"yarn start:server\" \"yarn start:client\"",
    "start:server": "cd backend && fastapi dev run.py",
    "start:client": "cd frontend && yarn dev"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "typescript": "^5.5.2"
  }
}
